{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vpnType": {
          "type": "string",
          "metadata": {
            "description": "Route based or policy based"
          },
          "defaultValue": "RouteBased",
          "allowedValues": [
            "RouteBased"
          ]
        },
        "localGatewayName": {
          "value": "localGateway"
        },
        "localGatewayIpAddress": {
          "value": "1.1.1.1"
        },
        "localAddressPrefix": {
          "value":"10.100.100.0/24"
        },
        "virtualNetworkName": {
          "value": "ApplicationvNet",
        },
        "azureVNetAddressPrefix": {
          "value": "10.100.102.9/23"
        },
        "subnetName": {
          "value": "ApplicationSubnet"
        },
        "subnetPrefix": {
          "value": "10.100.102.0/24",
        },
        "gatewaySubnetPrefix": {
          "value": "10.100.103.0/24",
        },
        "gatewayPublicIPName": {
          "value": "Azure-GatewayPublicIP"
        },
        "gatewayName": {
          "value": "Azure-Gateway"
        },
        "gatewaySku": {
          "value": "Basic"
        },
        "connectionName": {
          "value": "Azure-Gateway-AzureStack-S2SGateway"
        },
        "sharedKey": {
          "value": "securestring"
        }
      }
  }
